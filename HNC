    <!-- JavaScriptä»£ç  -->
    <script>
        // åˆå§‹åŒ–æ»‘å—å€¼æ˜¾ç¤º
        document.querySelectorAll('input[type="range"]').forEach(slider => {
            const valueSpan = document.getElementById(slider.id + 'Value');
            valueSpan.textContent = slider.value;
            
            slider.addEventListener('input', function() {
                valueSpan.textContent = this.value;
            });
        });

        // é£é™©è®¡ç®—å‡½æ•° - åŸºäºé€»è¾‘å›å½’æ¨¡å‹çš„ç®€åŒ–è®¡ç®—
        function calculateRisk() {
            // è·å–è¾“å…¥å€¼
            const pain = parseInt(document.getElementById('pain').value);
            const radiation = parseInt(document.getElementById('radiation').value);
            const stage = parseInt(document.getElementById('stage').value);
            const support = parseInt(document.getElementById('support').value);
            const nutrition = parseInt(document.getElementById('nutrition').value);
            const hemoglobin = parseInt(document.getElementById('hemoglobin').value);
            const depression = parseInt(document.getElementById('depression').value);
            const sleep = parseInt(document.getElementById('sleep').value);
            
            // æ ¹æ®ä½ çš„é€»è¾‘å›å½’æ¨¡å‹ç³»æ•°è®¡ç®—æ€»åˆ†
            // æ³¨æ„ï¼šè¿™æ˜¯ç®€åŒ–ç‰ˆæœ¬ï¼Œå®é™…åº”æ ¹æ®ä½ çš„å›å½’æ–¹ç¨‹è®¡ç®—
            const totalScore = pain * 2.56 + 
                             radiation * 2.18 + 
                             stage * 2.09 + 
                             (12 - support) * 1.40 +  // ç¤¾ä¼šæ”¯æŒæ˜¯ä¿æŠ¤å› ç´ 
                             nutrition * 1.38 + 
                             (16 - hemoglobin) * 1.13 + // è¡€çº¢è›‹ç™½æ˜¯ä¿æŠ¤å› ç´ 
                             depression * 1.10 + 
                             sleep * 1.09;
            
            // æ ‡å‡†åŒ–åˆ°0-100åˆ†èŒƒå›´
            const normalizedScore = Math.min(100, Math.max(0, Math.round(totalScore * 100 / 120)));
            
            // è®¡ç®—é£é™©æ¦‚ç‡ï¼ˆç®€åŒ–é€»è¾‘å›å½’æ¦‚ç‡è®¡ç®—ï¼‰
            // å®é™…åº”è¯¥ä½¿ç”¨ï¼šP = 1 / (1 + e^(-(Î²0 + Î²1X1 + ... + Î²nXn)))
            // è¿™é‡Œä½¿ç”¨ç®€åŒ–çº¿æ€§æ˜ å°„
            const riskProbability = Math.min(99, Math.round(normalizedScore * 0.95));
            
            // ç¡®å®šé£é™©ç­‰çº§
            let riskLevel, riskClass, recommendations;
            
            if (normalizedScore <= 30) {
                riskLevel = "ä½é£é™©";
                riskClass = "risk-low";
                recommendations = `
                    <h5>å»ºè®®å¹²é¢„æªæ–½ï¼š</h5>
                    <ul style="margin-left: 20px;">
                        <li>å¸¸è§„éšè®¿ç›‘æµ‹</li>
                        <li>å¥åº·æ•™è‚²æŒ‡å¯¼</li>
                        <li>æ¯3ä¸ªæœˆè¯„ä¼°ä¸€æ¬¡</li>
                        <li>ç»´æŒå¥åº·ç”Ÿæ´»æ–¹å¼</li>
                    </ul>
                `;
            } else if (normalizedScore <= 70) {
                riskLevel = "ä¸­é£é™©";
                riskClass = "risk-medium";
                recommendations = `
                    <h5>å»ºè®®å¹²é¢„æªæ–½ï¼š</h5>
                    <ul style="margin-left: 20px;">
                        <li>åŠ å¼ºç—‡çŠ¶ç›‘æµ‹ï¼ˆæ¯4-8å‘¨ï¼‰</li>
                        <li>å¿ƒç†ç¤¾ä¼šæ”¯æŒå¹²é¢„</li>
                        <li>è¥å…»å’¨è¯¢ä¸æ”¯æŒ</li>
                        <li>é€‚åº¦è¿åŠ¨æŒ‡å¯¼</li>
                        <li>ç¡çœ è´¨é‡è¯„ä¼°ä¸å¹²é¢„</li>
                    </ul>
                `;
            } else {
                riskLevel = "é«˜é£é™©";
                riskClass = "risk-high";
                recommendations = `
                    <h5>å»ºè®®å¹²é¢„æªæ–½ï¼š</h5>
                    <ul style="margin-left: 20px;">
                        <li>ç«‹å³å»ºç«‹å¤šå­¦ç§‘å›¢é˜Ÿç®¡ç†</li>
                        <li>å¼ºåŒ–ç—‡çŠ¶æ§åˆ¶ï¼ˆç–¼ç—›ã€æŠ‘éƒç­‰ï¼‰</li>
                        <li>ä¸ªä½“åŒ–è¥å…»æ”¯æŒæ–¹æ¡ˆ</li>
                        <li>å¿ƒç†æ²»ç–—ä¸å¿…è¦æ—¶çš„è¯ç‰©æ²»ç–—</li>
                        <li>æ¯å‘¨éšè®¿è¯„ä¼°</li>
                    </ul>
                `;
            }
            
            // æ›´æ–°æ˜¾ç¤ºç»“æœ
            document.getElementById('totalScore').textContent = normalizedScore;
            document.getElementById('riskProbability').textContent = riskProbability + "%";
            document.getElementById('riskLevel').textContent = riskLevel;
            
            // æ›´æ–°é£é™©å¾½ç« 
            const riskBadge = document.getElementById('riskBadge');
            riskBadge.textContent = riskLevel;
            riskBadge.className = 'risk-badge ' + riskClass;
            
            // æ›´æ–°å»ºè®®
            document.getElementById('recommendations').innerHTML = recommendations;
            
            // æ˜¾ç¤ºç»“æœå®¹å™¨
            document.getElementById('resultContainer').classList.add('show');
            
            // æ»šåŠ¨åˆ°ç»“æœä½ç½®
            document.getElementById('resultContainer').scrollIntoView({ 
                behavior: 'smooth',
                block: 'start'
            });
        }

        // é¡µé¢åŠ è½½å®Œæˆåè‡ªåŠ¨è®¡ç®—ä¸€æ¬¡
        document.addEventListener('DOMContentLoaded', function() {
